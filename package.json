{
   "name": "@scrypted/neato",
   "version": "0.1.5",
   "description": "A Neato plugin for Scrypted",
   "author": "Scrypted",
   "license": "Apache",
   "scripts": {
      "prepublish": "NODE_ENV=production scrypted-webpack",
      "prescrypted-vscode-launch": "scrypted-webpack",
      "scrypted-vscode-launch": "scrypted-deploy-debug",
      "scrypted-deploy-debug": "scrypted-deploy-debug",
      "scrypted-debug": "scrypted-debug",
      "scrypted-deploy": "scrypted-deploy",
      "scrypted-webpack": "scrypted-webpack"
   },
   "keywords": [
      "neato",
      "scrypted",
      "plugin"
   ],
   "scrypted": {
      "name": "Neato Controller",
      "type": "Device",
      "interfaces": [
         "DeviceProvider",
         "OauthClient"
      ]
   },
   "devDependencies": {
      "@scrypted/sdk": "0.0.1",
      "exports-loader": "^0.7.0",
      "webpack-merge": "^4.2.1"
   },
   "dependencies": {
      "crypto-js": "^3.1.9-1",
      "node-botvac": "github:koush/node-botvac#4ec89aefe3527985aef4fb59d73a4f176f0a9385"
   }
}
